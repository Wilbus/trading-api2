namespace schwabMarketData;

table Candle
{
    open: double;
    high: double;
    low: double;
    close: double;
    volume: int64;
    datetime: int64;
    datetimeISO8601: string;
}

table CandleList
{
    candles: [Candle];
    empty: bool;
    previousClose: double;
    previousCloseDate: int64;
    previousCloseDateISO8601: string;
    symbol: string;
}

root_type CandleList;